<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.pivobar" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PIVOBAR</name>
    <description>
App desarrollada por gnuino
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
     GNUINO
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
     <icon src="www/assets/img/lpivologo-1.png" width="200px" height="200px" />
</widget>
